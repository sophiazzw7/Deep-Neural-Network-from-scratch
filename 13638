LIBNAME mylib "/sasuser/home/"; /* Adjust path based on your SAS environment */

* Step 1: Import Both Excel Files;
PROC IMPORT OUT=mylib.part1
    DATAFILE="/sasuser/home/part1.xlsx"
    DBMS=XLSX REPLACE;
    GETNAMES=YES;
RUN;

PROC IMPORT OUT=mylib.part2
    DATAFILE="/sasuser/home/part2.xlsx"
    DBMS=XLSX REPLACE;
    GETNAMES=YES;
RUN;

* Step 2: Merge the Two Datasets;
DATA mylib.merged_data;
    SET mylib.part1 mylib.part2;
RUN;

* Step 3: Export the Merged Dataset Back to Excel;
PROC EXPORT DATA=mylib.merged_data
    OUTFILE="/sasuser/home/merged_file.xlsx"
    DBMS=XLSX REPLACE;
RUN;
