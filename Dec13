/* 
   1) Identify all (event_id, post_dt) pairs with row_count>1 
      from your check_dups table.
   2) Extract the *detail rows* from event_w_exclusion for only 
      those pairs, so you can inspect them side by side.
*/
proc sql;
  create table duplicates_detail as
  select a.*
  from event_w_exclusion a
  inner join check_dups b 
     on  a.'Internal Events'n = b.event_id
     and a.'Posting Date'n    = b.post_dt
  where b.row_count > 1
  order by a.'Internal Events'n, a.'Posting Date'n;
quit;
proc print data=duplicates_detail (obs=50);
  var 'Internal Events'n 'Posting Date'n 'GL Tracking ID'n 
      'Gross Loss Amount'n 'Recovery Amount'n net_loss
      'GL Account'n /* plus any other columns that matter */;
run;
